            <Menu mnemonicParsing="false" text="Keluar">
              <items>
                <MenuItem fx:id="mnukeluar" mnemonicParsing="false" onAction="#keluarclick" text="Keluar" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
