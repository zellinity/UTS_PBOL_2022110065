<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="pengolahannilai.FXML_menuController">
   <children>
      <MenuBar layoutY="-2.0" prefHeight="28.0" prefWidth="608.0">
        <menus>
          <Menu mnemonicParsing="false" text="Master">
            <items>
              <MenuItem fx:id="mnumahasiswa" mnemonicParsing="false" onAction="#mastermahasiswaclick" text="Mahasiswa" />
                  <MenuItem fx:id="mnumatkul" mnemonicParsing="false" onAction="#mastermatkulclick" text="Matakuliah" />
                  <MenuItem fx:id="mnudosen" mnemonicParsing="false" onAction="#masterdosenclick" text="Dosen" />
